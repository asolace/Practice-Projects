{
  "name": "JavaScript-Stack",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/asolace/JavaScript-Stack.git",
  "author": "asolace <jackhe678@gmail.com>",
  "scripts": {
    "start": "gulp",
    "test": "gulp test",
    "tutorial-test": "gulp main"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "flowtype"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "flow"
    ],
    "compact": false
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "chai": "^3.5.0",
    "immutable": "^3.8.1",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-immutable": "^3.1.0"
  },
  "devDependencies": {
    "babel-loader": "^6.3.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "del": "^2.2.2",
    "eslint": "*",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-flowtype": "^1.0.0",
    "gulp-mocha": "^4.0.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "webpack-stream": "^3.2.0"
  }
}
